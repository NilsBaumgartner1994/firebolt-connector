{
  "name": "firebolt-connector",
  "version": "0.0.42",
  "description": "Firebolt-API",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "coverage": "jest --coverage && cat ./build/coverage/lcov.info | coveralls",
    "test": "pwd && npm run build && npm run testOnly",
    "testOnly": "npm run coverage",
    "start": "npm run build && node ./build/development.js",
    "_prepareBuild": "rm -rf ./build",
    "build": "npm run _prepareBuild && rimraf ./build && tsc && cp package.json ./build && cp README.md ./build",
    "dev": "nodemon",
    "_prepareUpload": "npm run build && rm -rf ./build/coverage && rm -rf ./build/tests",
    "_uploadBuild": "cd ./build && npm version patch && npm publish && cd ..",
    "upload": "npm run _prepareUpload && npm run _uploadBuild && npm version patch && npm run clean",
    "clean": "rm -rf ./build && gts clean",
    "lint": "gts lint",
    "fix": "gts fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NilsBaumgartner1994/firebolt-connector.git"
  },
  "keywords": [
    "firebolt",
    "api"
  ],
  "author": "Nils Baumgartner",
  "contributors": [
    {
      "name": "Nils Baumgartner",
      "email": "nilsbaumgartner1994@gmail.com",
      "url": "https://github.com/NilsBaumgartner1994"
    },
    {
      "name": "Steffen Droppelmann",
      "email": "",
      "url": ""
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/NilsBaumgartner1994/firebolt-connector/issues"
  },
  "homepage": "https://github.com/NilsBaumgartner1994/firebolt-connector#readme",
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/node": "^16.0.0",
    "coveralls": "^3.1.1",
    "jest": "^27.0.6",
    "nodemon": "^2.0.9",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.5",
    "gts": "^3.1.0"
  },
  "dependencies": {
    "@babel/preset-typescript": "^7.14.5"
  }
}
